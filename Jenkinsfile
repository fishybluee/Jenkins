pipeline{
    agent any
    stages{
        stage('dev')
        {
            steps{
                echo "hello world"
            }
        
        }
        stage('level1')
        {
            steps{
                echo "Reached level1"
            }
        }   
        stage('level2')
        {
            steps{
                echo "Reached level2"
            }
        }    
        stage('level3')
        {
            steps{
                echo "Reached level3"
            }
        }  
        
    }        
}